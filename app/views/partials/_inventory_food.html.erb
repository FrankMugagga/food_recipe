<ul class='inv_food_head'>
  <li class='inv_food_lis'> <%= inventory.created_at %> </li>
  <li class='inv_food_lis'> <%= inventory.food.name %> </li>
  <li class='inv_food_lis'> <%= inventory.quantity %> <%= inventory.food.measurement_unit %></li>
  <% if can? :manage, inventory %>
    <li class='inv_food_lis'> <%= link_to 'Delete', inventory_inventory_food_path(@inventory, inventory), method: :delete, data: { turbo_method: :delete, turbo_confirm: 'Are you sure?' } %> </li>
  <% else %>
   <li class='inv_food_lis'> No authorised action</li>
  <% end %>
</ul>